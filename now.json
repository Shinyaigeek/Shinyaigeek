{
    "builds": [
      {
        "src": "/express.ts",
        "use": "@now/node-server"
      },
      {
          "src": "/dist/*.js",
          "use": "@now/static"
      },
      {
          "src": "/public/*.css",
          "use": "@now/static"
      },
      {
          "src": "/static/*.png",
          "use": "@now/static"
      }
    ],
    "routes": [
        {
            "src": "/*.js",
            "dest": "dist/"
        },
        {
            "src": "/*.png",
            "dest": "static/"
        },
        {
            "src": "/.*",
            "dest": "express.ts"
        }
    ],
    "env": {
        "CONTENTFUL_SPACE_ID": "@contentful-space-id-production-4lm2",
        "CONTENTFUL_ACCESS_TOKEN": "@contentful-access-token-production-ra4w"
    }
}