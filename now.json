{
    "builds": [
      {
        "src": "./express.ts",
        "use": "@now/node-server"
      }
    ],
    "routes": [
        {
            "src": "/*.js",
            "dest": "dist/"
        },
        {
            "src": "/*.png",
            "dest": "static/"
        },
        {
            "src": "/.*",
            "dest": "dist/express.js"
        }
    ]
}