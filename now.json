{
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "/dist/*.js",
            "use": "@now/static"
        },
        {
            "src": "/public/*.css",
            "use": "@now/static"
        },
        {
            "src": "/static/*.png",
            "use": "@now/static"
        },
        {
            "src": "/express.ts",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/*.js",
            "dest": "dist/$1.js"
        },
        {
            "src": "/*.css",
            "dest": "dist/$1.css"
        },
        {
            "src": "/*.png",
            "dest": "static/$1.png"
        },
        {
            "src": "/.*",
            "dest": "express.ts"
        }
    ],
    "env": {
        "CONTENTFUL_SPACE_ID": "@contentful-space-id-production-4lm2",
        "CONTENTFUL_ACCESS_TOKEN": "@contentful-access-token-production-ra4w"
    }
}